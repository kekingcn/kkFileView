(6[0-9])([0-9][0-9]) $1शे $2
(7[0-9])([0-9][0-9]) $1शे $2
(8[0-9])([0-9][0-9]) $1शे $2
(9[0-9])([0-9][0-9]) $1शे $2
(.*) $(year-remove-and $1)

